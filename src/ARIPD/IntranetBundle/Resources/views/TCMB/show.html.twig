<h1>TCMB</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Date</th>
            <td>{{ entity.date|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Code</th>
            <td>{{ entity.code }}</td>
        </tr>
        <tr>
            <th>Forexbuying</th>
            <td>{{ entity.forexbuying }}</td>
        </tr>
        <tr>
            <th>Forexselling</th>
            <td>{{ entity.forexselling }}</td>
        </tr>
        <tr>
            <th>Banknotebuying</th>
            <td>{{ entity.banknotebuying }}</td>
        </tr>
        <tr>
            <th>Banknoteselling</th>
            <td>{{ entity.banknoteselling }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('tcmb') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('tcmb_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('tcmb_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
