{% extends 'ARIPDAdminBundle::layout.html.twig' %}

{% block container %}
<div class="page-header">
	<h1>{{ 'Suppliers'|trans({}, 'ARIPDSCMBundle') }}</h1>
</div>
<form action="{{ path('aripd_admin_scm_supplier_create') }}" method="post" {{ form_enctype(form) }}>
    {{ form_widget(form) }}
    <p>
        <button type="submit">Create</button>
    </p>
</form>

<ul class="record_actions">
    <li>
        <a href="{{ path('aripd_admin_scm_supplier_index') }}">
            Back to the list
        </a>
    </li>
</ul>
{% endblock %}