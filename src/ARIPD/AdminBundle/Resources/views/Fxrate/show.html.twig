<table class="table">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Date</th>
            <td>{{ entity.date|localizeddate('short', 'medium') }}</td>
        </tr>
        <tr>
            <th>Code</th>
            <td>{{ entity.code }}</td>
        </tr>
        <tr>
            <th>Forexbuying</th>
            <td>{{ entity.forexBuying }}</td>
        </tr>
        <tr>
            <th>Forexselling</th>
            <td>{{ entity.forexSelling }}</td>
        </tr>
        <tr>
            <th>Banknotebuying</th>
            <td>{{ entity.banknoteBuying }}</td>
        </tr>
        <tr>
            <th>Banknoteselling</th>
            <td>{{ entity.banknoteSelling }}</td>
        </tr>
    </tbody>
</table>

<div class="form-actions">
	<a class="btn" href="{{ path('aripd_admin_fxrate_index') }}"><i class="icon-chevron-left"></i> {{ 'Back'|trans() }}</a>
	<a class="btn" href="{{ path('aripd_admin_fxrate_edit', { 'id': entity.id }) }}"><i class="icon-edit"></i> {{ 'Edit'|trans() }}</a>
	<button class="btn btn-danger" type="button" onclick="deleteRecord();"><i class="icon-trash"></i> {{ 'Delete'|trans() }}</button>
</div>

<form id="delete_form" action="{{ path('aripd_admin_fxrate_delete', { 'id': entity.id }) }}" method="post">
	{{ form_widget(delete_form) }}
</form>